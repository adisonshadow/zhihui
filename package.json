{
  "name": "yiman",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite --config vite.config.web.ts",
    "build": "vite build && electron-builder",
    "test": "vitest",
    "postinstall": "electron-rebuild"
  },
  "build": {
    "appId": "com.yiman.app",
    "productName": "芝绘",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "dist-electron",
      "electron/models/**"
    ],
    "mac": {
      "category": "public.app-category.entertainment",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/x": "^2.2.1",
    "@ant-design/x-markdown": "^2.2.1",
    "@ant-design/x-sdk": "^2.2.1",
    "@dnd-kit/accessibility": "^3.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@volcengine/sdk-core": "^1.0.0",
    "@wavesurfer/react": "^1.0.12",
    "adm-zip": "^0.5.16",
    "antd": "^6.2.2",
    "better-sqlite3": "^12.6.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "delaunator": "^5.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "js-yaml": "^4.1.1",
    "onnxruntime-node": "^1.24.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "sharp": "^0.34.5",
    "wavesurfer.js": "^7.12.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.18"
  }
}
